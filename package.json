{
  "name": "my-electron-app",
  "version": "1.0.0",
  "description": "A simple Electron application.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build",
    "dist:win": "npm run dist -- -w",
    "dist:mac": "npm run dist -- -m",
    "dist:linux": "npm run dist -- -l",
    "lint": "eslint --config .eslintrc.json ."
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "adm-zip": "^0.5.12",
    "discord-rpc-patch": "^4.0.1",
    "ejs": "^3.1.10",
    "ejs-electron": "^3.0.0",
    "electron-updater": "^6.1.8",
    "fs-extra": "^11.1.1",
    "github-syntax-dark": "^0.5.0",
    "got": "^11.8.5",
    "jquery": "^3.7.1",
    "lodash.merge": "^4.6.2",
    "semver": "^7.6.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0"
  },
  "build": {
    "appId": "com.example.my-electron-app",
    "mac": {
      "category": "public.app-category.utilities",
      "target": ["dmg", "zip"]
    },
    "win": {
      "target": ["nsis", "msi"]
    },
    "linux": {
      "target": ["AppImage", "deb"]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Abhinav2011VS/Electron-MultiPlatform-Apps.git"
  },
  "author": "Your Name",
  "license": "UNLICENSED"
}
